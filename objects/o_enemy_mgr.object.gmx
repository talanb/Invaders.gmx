    with (e) {
        image_index = !image_index    
        if dir &gt; 0 {
            if right_x + (sprite_get_width(sprite_index)/2) + 2 &gt; 550 {
                show_debug_message("Left edge - Will move down: " + scr_format_coordinate(o_enemy_mgr.next_row_to_move, col))
                dir = -dir
                y += 8
                will_move_down = true
            }
        } else {
            if left_x - (sprite_get_width(sprite_index)/2) - 2 &lt;= 60 {
                show_debug_message("Right edge - Will move down: " + scr_format_coordinate(o_enemy_mgr.next_row_to_move, col))
                dir = -dir
                y += 8
                will_move_down = true
            }
        }
        if (!will_move_down)
           x += dir * 2
    }
}
next_row_to_move = scr_get_next_row_to_move(next_row_to_move)
alarm[0] = room_speed / 10
}
</string>
